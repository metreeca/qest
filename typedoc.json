{
  "plugin": [
    "typedoc-github-theme"
  ],
  "projectDocuments": [
    "pub/*.md"
  ],
  "externalSymbolLinkMappings": {
    "@metreeca/core": {
      "IRI": "https://metreeca.github.io/core/types/resource.IRI.html",
      "Tag": "https://metreeca.github.io/core/types/language.Tag.html",
      "Range": "https://metreeca.github.io/core/types/language.Range.html"
    }
  },
  "includeVersion": true,
  "sortEntryPoints": false,
  "sort": [
    "source-order"
  ],
  "groupOrder": [
    "Variables",
    "Type Aliases",
    "*"
  ],
  "useFirstParagraphOfCommentAsSummary": true,
  "favicon": "https://metreeca.com/assets/icon.ico",
  "sourceLinkExternal": true,
  "markdownLinkExternal": true,
  "navigationLinks": {
    "GitHub": "https://github.com/metreeca/qest"
  },
  "hideGenerator": true,
  "customFooterHtml": "Copyright Â© 2025 <a href='https://www.metreeca.com/'><img src='https://metreeca.com/assets/icon.svg' alt='Metreeca' style='width: 1.25em; height: 1.25em; vertical-align: bottom;'/> Metreeca</a>",
  "headings": {
    "readme": false
  }
}
