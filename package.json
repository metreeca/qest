{
  "name": "@metreeca/qest",
  "version": "0.9.0",
  "description": "Minimalist foundations for client-driven, queryable REST/JSON APIs",
  "keywords": [
    "typescript",
    "rest",
    "json",
    "json-ld",
    "query",
    "api"
  ],
  "homepage": "https://metreeca.github.io/qest/",
  "bugs": {
    "url": "https://github.com/metreeca/qest/issues",
    "email": "info@metreeca.com"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Metreeca",
    "email": "info@metreeca.com",
    "url": "https://www.metreeca.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metreeca/qest.git"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./state": "./dist/state.js",
    "./query": "./dist/query.js"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist docs node_modules src/codec.pegjs.*",
    "setup": "npm install && npm audit fix",
    "peggy": "npx peggy --dts --format es --output src/codec.pegjs.js --allowed-start-rules '*' src/codec.pegjs",
    "build": "rm -rf dist docs && npm run peggy && tsc && typedoc",
    "check": "npx vitest run",
    "watch": "npx tsc --watch",
    "proof": "npx typedoc && (npx typedoc --watch & npx five-server docs --open)"
  },
  "dependencies": {
    "@metreeca/core": "^0.9.15"
  },
  "devDependencies": {
    "five-server": "^0.4.5",
    "peggy": "^4.1.3",
    "typedoc": "^0.28.14",
    "typedoc-github-theme": "^0.3.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  }
}
