{
  "name": "@metreeca/qest",
  "version": "0.9.1",
  "description": "Minimalist foundations for client-driven, queryable REST/JSON APIs",
  "keywords": [
    "typescript",
    "rest",
    "json",
    "json-ld",
    "query",
    "api"
  ],
  "homepage": "https://metreeca.github.io/qest/",
  "bugs": {
    "url": "https://github.com/metreeca/qest/issues",
    "email": "info@metreeca.com"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Metreeca",
    "email": "info@metreeca.com",
    "url": "https://www.metreeca.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metreeca/qest.git"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./state": "./dist/state.js",
    "./query": "./dist/query.js"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist docs node_modules 'src/$'",
    "setup": "npm install && npm audit fix",
    "typia": "npx typia generate --project tsconfig.json --output 'src/$' --input 'src/$typia'",
    "peggy": "npx peggy --dts --format es --allowed-start-rules '*' --output 'src/$/query.pegjs.js' 'src/$peggy/query.pegjs'",
    "build": "rm -rf dist docs && npm run typia && npm run peggy && tsc -p tsconfig.build.json && npx typedoc",
    "check": "npx vitest --typecheck run",
    "watch": "npx tsc --watch",
    "proof": "npx typedoc && (npx typedoc --watch & npx five-server docs --open)"
  },
  "dependencies": {
    "@metreeca/core": "^0.9.16"
  },
  "devDependencies": {
    "five-server": "^0.4.5",
    "peggy": "^4.1.3",
    "typia": "^10.1.0",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  }
}
